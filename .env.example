# Local example environment variables (do NOT commit real keys)
# Copy to .env and fill values

# Server-side Google Maps key (used by /api/distance route)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# If you also need client-side Maps JS, create NEXT_PUBLIC_GOOGLE_MAPS_API_KEY
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_public_google_maps_api_key_here

# Firebase (example placeholders)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
